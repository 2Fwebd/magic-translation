{
    "name": "alkalab/magic-translation",
    "description": "Automatically translate your Laravel localization files to any language using Google Translate through an Artisan command.",
    "keywords": ["google", "translate", "php", "automatic", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.5.*",
        "p3k/emoji-detector": "^0.2.1",
        "stichoza/google-translate-php": "^3.2"
    },
    "minimum-stability": "dev",
    "homepage": "https://github.com/2Fwebd/magic-translation",
    "authors": [
        {
            "name": "2Fwebd",
            "email": "help@2f-design.fr"
        }
    ],
    "autoload": {
        "psr-0": {
            "Alkalab\\MagicTranslate\\": "src/"
        },
        "psr-4": {
            "Alkalab\\MagicTranslate\\": "src/Alkalab/MagicTranslate/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alkalab\\MagicTranslate\\ServiceProvider"
            ]
        }
    }
}
